 */

/**
 * @author jebediah
 *
 */
public class IAm implements Runnable {

	private String str;
	Thread t;
	/* (non-Javadoc)
	 * @see java.lang.Runnable#run()
	 */
	public IAm(String str)
	{
		this.str = str;
		t = new Thread(this);
		t.start();
	}
	@Override
	public void run() {
		// TODO Auto-generated method stub
		Thread.sleep(500);
		
		for (int i = 0; i < 9000; i++)
		{
			Thread.sleep(1);
			System.out.println(str);
		}
	}
}

