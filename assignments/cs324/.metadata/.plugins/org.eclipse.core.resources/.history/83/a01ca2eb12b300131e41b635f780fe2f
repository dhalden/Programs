
public class CarQSemaphore {
	private int q;
	private int maxcarqueue;
	
	
	public CarQSemaphore(int initq, int maxcars)
	{
		q = initq;
		maxcarqueue=maxcars;
		
	}
	public int synchronized getq()
	{
		return q;
	}
	
	public boolean synchronized arrival()
	{
		if(q < maxcarqueue)
		{
			q++;
			return true;
		}
		else
		{
			return false;
		}
	}
	
	public boolean synchronized departure()
	{
		if(q > 0)
		{
			q--;
			return true;
		}
		else
		{
			return false;
		}
	}

}
